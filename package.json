  "engines": {
    "node": "^16.16.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build limble-tree",
    "buildWatch": "ng build limble-tree --watch",
    "start": "ng serve sandbox --open",
    "linkStart": "npm run start",
    "test": "ng test",
    "lintCheck": "eslint projects/limble-tree/src/lib projects/sandbox/src/app --ext .js --ext .ts --ext .html",

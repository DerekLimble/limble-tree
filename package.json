{
   "name": "libs",
   "version": "0.11.0",
   "license": "MIT",
   "scripts": {
      "ng": "ng",
      "build": "ng build --prod limble-tree",
      "buildWatch": "ng build --prod limble-tree --watch",
      "link": "cd dist/limble-tree && npm link && cd ../.. && npm link @limble/limble-tree",
      "start": "ng serve sandbox --open",
      "linkStart": "npm run link && npm run start",
      "test": "ng test",
      "lintCheck": "eslint projects/limble-tree/src/lib projects/sandbox/src/app --ext .js --ext .ts --ext .html",
      "lintWatch": "esw projects/limble-tree/src/lib projects/sandbox/src/app --watch --ext .js --ext .ts --ext .html",
      "deepUpdate": "npm update && rm -rf node_modules/ && rm package-lock.json && npm install",
      "publishPackage": "npm run build && cd dist/limble-tree && npm publish --ignore-scripts"
   },
   "private": true,
   "dependencies": {
      "@angular/animations": "^11.2.5",
      "@angular/common": "^11.2.5",
      "@angular/compiler": "^11.2.5",
      "@angular/core": "^11.2.5",
      "@angular/forms": "^11.2.5",
      "@angular/platform-browser": "^11.2.5",
      "@angular/platform-browser-dynamic": "^11.2.5",
      "@angular/router": "^11.2.5",
      "@limble/limble-tree": "*",
      "rxjs": "~6.6.6",
      "tslib": "^2.0.0",
      "zone.js": "~0.10.2"
   },
   "devDependencies": {
      "@angular-devkit/build-angular": "^0.1102.4",
      "@angular-eslint/builder": "~1.2.0",
      "@angular-eslint/eslint-plugin": "~1.2.0",
      "@angular-eslint/eslint-plugin-template": "~1.2.0",
      "@angular-eslint/schematics": "~1.2.0",
      "@angular-eslint/template-parser": "~1.2.0",
      "@angular/cli": "^11.2.4",
      "@angular/compiler-cli": "^11.2.5",
      "@types/jasmine": "^3.6.6",
      "@types/node": "^12.20.5",
      "@typescript-eslint/eslint-plugin": "^4.17.0",
      "@typescript-eslint/parser": "^4.17.0",
      "eslint": "^7.21.0",
      "eslint-plugin-eslint-comments": "^3.2.0",
      "eslint-watch": "^7.0.0",
      "jasmine-core": "~3.6.0",
      "jasmine-spec-reporter": "~5.0.0",
      "karma": "~5.1.0",
      "karma-chrome-launcher": "~3.1.0",
      "karma-coverage": "~2.0.3",
      "karma-jasmine": "~4.0.0",
      "karma-jasmine-html-reporter": "^1.5.0",
      "ng-packagr": "^11.2.4",
      "prettier": "^2.2.1",
      "protractor": "~7.0.0",
      "ts-node": "~8.3.0",
      "typescript": "~4.1.5"
   },
   "prettier": {
      "printWidth": 80,
      "tabWidth": 3,
      "useTabs": false,
      "semi": true,
      "singleQuote": false,
      "quoteProps": "consistent",
      "trailingComma": "none",
      "bracketSpacing": true,
      "arrowParens": "always",
      "proseWrap": "preserve",
      "htmlWhitespaceSensitivity": "css",
      "endOfLine": "lf"
   }
}
